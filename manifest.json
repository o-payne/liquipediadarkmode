{
  "short_name": "Liquipedia Dark",
  "name": "Liquipedia Dark Mode",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/logo-medium16.png",
    "48": "icons/logo-medium16.png",
    "128": "icons/logo-medium128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://www.liquipedia.net/*",
    "https://liquipedia.net/*",
    "storage"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Liquipedia Dark"
  },
  "version": "1.0"
}
